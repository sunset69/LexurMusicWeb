.full {
    width: 100%;
    height: 100vh;
    background-color: rgb(255, 118, 118);
    display: flex;
    align-items: center;
    justify-content: center;
}
.content {
    /* border: 1px black solid; */
    border-radius: 10px;
    width: 320px;
    height: 400px;
    box-shadow: 0 1px 50px rgb(2 2 2 / 30%);
}
.tittle {
    text-align: center;
    margin: 10px;
    font-size: 50px;
    color: bisque;
}

